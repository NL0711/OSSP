orders = [[4, 3, 0], [3, 8, 1], [1, 6, 2], [5, 4, 4], [2, 2, 5],]

ready_queue = []

time = 0
for order in orders:
  if order[2] <= time:
    ready_queue.append(order)

if ready_queue:

    #sort by shortest burst time
    for i in range (len(ready_queue)-1):
      if ready_queue[i][1] > ready_queue[i+1][1]:
        ready_queue[i], ready_queue[i+1] = ready_queue[i+1], ready_queue[i]

print(ready_queue)
